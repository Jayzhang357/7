<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="text_normal">

        <item name="android:textStyle">bold</item>
        <item name="android:textSize">40dp</item>
        <item name="android:textColor">@color/backset</item>
    </style>
    <style name="text_Big">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">50dp</item>
        <item name="android:textColor">@color/backset</item>
    </style>
    <style name="text_Small">

        <item name="android:textStyle">bold</item>
        <item name="android:textSize">30dp</item>
        <item name="android:textColor">@color/backset</item>
    </style>
    <style name="text_verySmall">

        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@color/backset</item>
    </style>
    <style name="gnss_setting_button">
        <item name="android:background">@drawable/back_g2</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:textSize">18sp</item>

        <item name="android:stateListAnimator" tools:targetApi="lollipop">@null</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="setnt_button">
        <item name="android:background">@drawable/back_g2</item>

        <item name="android:textSize">18sp</item>

        <item name="android:stateListAnimator" tools:targetApi="lollipop">@null</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="gnss_setting_button_b">
        <item name="android:background">@drawable/back_g2</item>

        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:stateListAnimator" tools:targetApi="lollipop">@null</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="APNEditTextQQQ">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">0.8</item>
        <item name="android:textSize">8pt</item>
        <item name="android:textColor">@color/backset</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/ic_edittext</item>
        <item name="android:lines">1</item>
        <item name="android:imeOptions">actionDone</item>
    </style>
    <style name="edittext_name">
        <item name="android:background">@drawable/solid_bg</item>
        <item name="android:lines">1</item>
        <item name="android:gravity">center</item>
        <item name="android:ems">10</item>
        <item name="android:maxLength">20</item>
        <item name="android:textSize">10pt</item>
        <item name="android:imeOptions">actionDone</item>
    </style>

    <style name="edittext_namecs">
        <item name="android:background">@drawable/solid_bg</item>
        <item name="android:lines">1</item>
        <item name="android:gravity">center</item>
        <item name="android:ems">10</item>
        <item name="android:textColor">@color/backset</item>
        <item name="android:maxLength">20</item>
        <item name="android:textSize">10pt</item>
        <item name="android:imeOptions">actionDone</item>
    </style>
    <style name="gnss_setting_button_s">
        <item name="android:background">@drawable/back_g2</item>
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:stateListAnimator" tools:targetApi="lollipop">@null</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="page_vehicle_table">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">#d3d7d4</item>
    </style>
    <style name="page_vehicle_tablecell">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/textback</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="imaginview_btn_bottom">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>

        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>

    </style>
    <style name="loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/dialog_main</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="long_btn">
        <item name="android:background">@drawable/selector_normal_btn</item>
        <item name="android:layout_width">196dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textSize">18sp</item>

    </style>
    <style name="common_grid_style">
        <item name="android:fadeScrollbars">false</item>
        <item name="android:gravity">center</item>
        <item name="android:numColumns">auto_fit</item>
        <item name="android:scrollbarAlwaysDrawVerticalTrack">true</item>
        <item name="android:scrollbarFadeDuration">-1</item>
        <item name="android:scrollbarSize">10dp</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:stretchMode">columnWidth</item>
        <item name="android:verticalSpacing">2dp</item>
        <item name="android:listSelector">@drawable/timer_list_selector</item>

    </style>
</resources>