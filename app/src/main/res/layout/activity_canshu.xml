<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/back">

    <RelativeLayout
        android:id="@+id/root1"
        android:layout_width="fill_parent"
        android:layout_height="80dp"

        android:layout_marginTop="20dp">


        <RadioGroup
            android:id="@+id/radioGroup"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:orientation="horizontal">


            <RadioButton
                android:id="@+id/r1"
                style="@style/text_normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/czcs" />

            <RadioButton
                android:id="@+id/r2"
                style="@style/text_normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100dp"
                android:layout_toRightOf="@+id/r1"
                android:text="@string/bksz" />


            <RadioButton
                android:id="@+id/r3"
                style="@style/text_normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100dp"
                android:layout_toRightOf="@+id/r1"
                android:text="@string/cansz" />
        </RadioGroup>
    </RelativeLayout>

    <View
        android:id="@+id/v1"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_below="@+id/root1"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:background="@android:color/white" />


    <RelativeLayout
        android:id="@+id/root2"
        android:layout_width="fill_parent"
        android:layout_height="400dp"
        android:layout_below="@+id/v1"

        android:visibility="visible">

      <LinearLayout
          android:orientation="horizontal"
          android:layout_marginLeft="60dp"
          android:layout_marginRight="60dp"
          android:layout_marginTop="20dp"
          android:layout_width="fill_parent"
          android:layout_height="180dp">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="200dp"
            android:layout_height="fill_parent">
            <TextView
                android:id="@+id/kzjdt"
                style="@style/text_Small"
                android:layout_width="match_parent"
                android:layout_height="50dp"

                android:text="@string/kzjd" />

            <TextView
                android:id="@+id/kzlmdt"
                style="@style/text_Small"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"

                android:text="@string/kzlmd" />
            <TextView
                android:id="@+id/wdcst"
                style="@style/text_Small"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"

                android:text="@string/wdcs" />

        </LinearLayout>
          <LinearLayout
              android:orientation="vertical"
              android:layout_width="100dp"

              android:layout_height="fill_parent">
              <EditText
                  android:id="@+id/kzzd"
                  style="@style/edittext_namecs"
                  android:layout_width="100dp"
                  android:layout_height="50dp"
                  android:background="@color/textback"
                  android:inputType="number" />
              <EditText
                  android:id="@+id/kzlmd"
                  style="@style/edittext_namecs"
                  android:layout_width="100dp"
                  android:layout_height="50dp"
                  android:layout_marginTop="10dp"
                  android:background="@color/textback"
                  android:inputType="number" />
              <EditText
                  android:id="@+id/wdcs"
                  style="@style/edittext_namecs"
                  android:layout_width="100dp"
                  android:layout_height="50dp"
                  android:layout_marginTop="10dp"
                  android:background="@color/textback"
                  android:inputType="number" />


          </LinearLayout>
        <LinearLayout
            android:layout_marginLeft="40dp"
            android:orientation="vertical"
            android:layout_width="2dp"
            android:background="@color/white"
            android:layout_height="fill_parent"/>

          <LinearLayout
              android:layout_marginLeft="40dp"
              android:orientation="vertical"
              android:layout_width="100dp"
              android:layout_height="fill_parent">
              <TextView

                  style="@style/text_Small"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"

                  android:text="@string/ycxz" />
              <TextView

                  style="@style/text_Small"
                  android:layout_marginTop="10dp"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"


                  android:text="@string/sdwxs" />
          </LinearLayout>
          <LinearLayout
              android:orientation="vertical"
              android:layout_width="100dp"
              android:layout_height="fill_parent">
              <EditText
                  android:id="@+id/jiaodu1_min"
                  style="@style/edittext_namecs"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"
                  android:background="@color/textback"
                  android:inputType="numberSigned" />

              <EditText
                  android:id="@+id/jiaodu2_min"
                  style="@style/edittext_namecs"
                  android:layout_marginTop="10dp"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"
                 android:background="@color/textback"
                  android:inputType="numberSigned" />

          </LinearLayout>
          <LinearLayout
              android:layout_marginLeft="10dp"
              android:orientation="vertical"
              android:layout_width="40dp"
              android:layout_height="fill_parent">
              <TextView
                  style="@style/text_Small"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"
                  android:gravity="center"
                  android:text="@string/d" />
              <TextView
                  style="@style/text_Small"
                  android:layout_marginTop="10dp"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"
                  android:gravity="center"
                  android:text="@string/d"  />
          </LinearLayout>
          <LinearLayout
              android:orientation="vertical"
              android:layout_width="100dp"
              android:layout_height="fill_parent">
              <EditText
                  android:id="@+id/jiaodu1_max"
                  style="@style/edittext_namecs"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"
                  android:background="@color/textback"
                  android:inputType="number" />

              <EditText
                  android:id="@+id/jiaodu2_max"
                  style="@style/edittext_namecs"
                  android:layout_marginTop="10dp"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"
                  android:background="@color/textback"
                  android:inputType="number" />

          </LinearLayout>
          <LinearLayout
              android:layout_marginLeft="10dp"
              android:orientation="vertical"
              android:layout_width="100dp"
              android:layout_height="fill_parent">
              <TextView
                  android:id="@+id/ycxzt"
                  style="@style/text_Small"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"
                  android:textColor="@color/blue"
              />
              <TextView
                  android:id="@+id/sdwsxt"
                  style="@style/text_Small"
                  android:layout_marginTop="10dp"
                  android:layout_width="match_parent"
                  android:layout_height="50dp"
                  android:textColor="@color/blue"
               />
          </LinearLayout>
      </LinearLayout>


        <Button
            android:id="@+id/hfmrcs"
            style="@style/gnss_setting_button_b"
            android:layout_width="200dp"
            android:layout_height="70dp"


            android:layout_marginLeft="200dp"
            android:layout_marginTop="280dp"
            android:focusable="false"
            android:text="@string/hfmrcs" />

        <Button
            android:id="@+id/save"
            style="@style/gnss_setting_button_b"
            android:layout_width="200dp"
            android:layout_height="70dp"

            android:layout_alignParentRight="true"
            android:layout_marginTop="280dp"
            android:layout_marginRight="200dp"
            android:focusable="false"
            android:text="@string/save" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/root3"
        android:layout_width="fill_parent"
        android:layout_height="400dp"
        android:layout_below="@+id/v1"
        android:layout_marginTop="20dp"
        android:visibility="invisible">

        <RelativeLayout
            android:id="@+id/rll04"
            android:layout_width="fill_parent"
            android:layout_height="250dp"

            android:layout_gravity="center"
            android:layout_marginTop="0dp"
            android:background="@color/back">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@color/back"
                android:orientation="horizontal"
                android:weightSum="2">

                <ScrollView

                    android:id="@+id/scroll"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="50dp"
                    android:layout_marginRight="50dp"
                    android:layout_weight="1"
                    android:background="@android:drawable/editbox_background_normal"

                    android:fadeScrollbars="false">

                    <TextView
                        android:id="@+id/TextViewReception"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:isScrollContainer="true"
                        android:scrollbarStyle="insideOverlay"
                        android:textColor="@color/black" />
                </ScrollView>


            </LinearLayout>
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rll05"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_below="@+id/rll04"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="5dp"
            android:layout_weight="2.61"
            android:background="@color/back"
            android:gravity="center_vertical"
            android:padding="10dp">


            <TextView
                android:id="@+id/textView1"
                style="@style/text_verySmall"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="30dp"
                android:gravity="center"
                android:text="@string/t2_tx" />

            <Spinner
                android:id="@+id/spinner1"
                style="@style/text_verySmall"
                android:layout_width="wrap_content"
                android:layout_height="60dp"

                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/textView1"
                android:enabled="false"
                android:gravity="center"
                android:spinnerMode="dropdown">


            </Spinner>

            <Button
                android:id="@+id/buSet"
                style="@style/gnss_setting_button"
                android:layout_width="100dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:layout_toRightOf="@+id/spinner1"
                android:text="@string/save" />

            <EditText
                android:id="@+id/EditTextEmission"
                android:layout_width="250dp"
                android:layout_height="60dp"

                android:layout_marginRight="10dp"
                android:layout_toLeftOf="@+id/sendbt"
                android:ems="10"
                android:lines="1"
                android:text="$read version">

            </EditText>

            <Button
                android:id="@+id/sendbt"
                style="@style/gnss_setting_button"
                android:layout_width="100dp"
                android:layout_height="60dp"
                android:layout_marginRight="10dp"

                android:layout_toLeftOf="@+id/btnPasue"
                android:text="@string/send" />

            <Button
                android:id="@+id/btnPasue"
                style="@style/gnss_setting_button"
                android:layout_width="100dp"
                android:layout_height="60dp"
                android:layout_alignParentRight="true"

                android:layout_marginRight="40dp"
                android:text="@string/pause" />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/root4"
        android:layout_width="fill_parent"
        android:layout_height="400dp"
        android:layout_below="@+id/v1"
        android:layout_marginTop="20dp"
        android:visibility="invisible">

        <RelativeLayout
            android:id="@+id/rll06"
            android:layout_width="fill_parent"
            android:layout_height="230dp"

            android:layout_gravity="center"
            android:layout_marginTop="0dp"
            android:background="@color/back">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@color/back"
                android:orientation="horizontal"
                android:weightSum="2">

                <ScrollView

                    android:id="@+id/scrollcan"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="50dp"
                    android:layout_marginRight="50dp"
                    android:layout_weight="1"
                    android:background="@android:drawable/editbox_background_normal"

                    android:fadeScrollbars="false">

                    <TextView
                        android:id="@+id/TextViewReceptioncan"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:isScrollContainer="true"
                        android:scrollbarStyle="insideOverlay"
                        android:textColor="@color/black" />
                </ScrollView>


            </LinearLayout>
        </RelativeLayout>


        <LinearLayout
            android:id="@+id/ll_send"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rll06"
            android:layout_marginLeft="50dp"
            android:gravity="bottom"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:orientation="horizontal">

                <Spinner
                    android:id="@+id/sp_channel"
                    style="@style/text_verySmall"
                    android:layout_width="150dp"
                    android:layout_height="fill_parent"
                    android:gravity="center" />

                <Spinner
                    android:id="@+id/sp_baud"
                    style="@style/text_verySmall"
                    android:layout_width="150dp"
                    android:layout_height="fill_parent"
                    android:gravity="center" />

                <TextView
                    style="@style/text_verySmall"
                    android:layout_width="55dp"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="(Hex)Mask" />

                <EditText

                    android:id="@+id/et_mask"
                    android:layout_width="150dp"
                    android:layout_height="fill_parent"
                    android:digits="@string/hex"
                    android:gravity="center"
                    android:hint="ID(Hex)"
                    android:maxLength="8"
                    android:text="000000F1" />

                <CheckBox
                    android:id="@+id/gl"
                    style="@style/text_verySmall"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="@string/filter" />

                <CheckBox
                    android:id="@+id/cb_loop"
                    style="@style/text_verySmall"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="@string/loop" />

                <EditText
                    android:id="@+id/et_count"
                    android:layout_width="60dp"
                    android:layout_height="fill_parent"

                    android:layout_marginLeft="45dp"
                    android:gravity="center"
                    android:hint="10000000"
                    android:inputType="number"
                    android:maxLength="8"
                    android:text="1000" />

                <TextView
                    style="@style/text_verySmall"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="@string/count" />

                <Button
                    android:id="@+id/btn_ml"
                    style="@style/setnt_button"
                    android:layout_width="110dp"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="8dp"
                    android:gravity="center"
                    android:text="@string/scml" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"

                android:layout_height="60dp"
                android:orientation="horizontal">

                <Spinner
                    android:id="@+id/sp_idtype"
                    style="@style/text_verySmall"
                    android:layout_width="150dp"
                    android:layout_height="fill_parent"
                    android:gravity="center" />

                <Spinner
                    android:id="@+id/sp_frametype"
                    style="@style/text_verySmall"
                    android:layout_width="150dp"
                    android:layout_height="fill_parent"
                    android:gravity="center" />

                <TextView
                    style="@style/text_verySmall"
                    android:layout_width="55dp"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="(Hex)帧" />

                <EditText
                    android:id="@+id/et_id"
                    android:layout_width="100dp"
                    android:layout_height="fill_parent"
                    android:digits="@string/hex"
                    android:gravity="center"
                    android:hint="ID(Hex)"
                    android:maxLength="8"
                    android:text="28B " />

                <TextView
                    style="@style/text_verySmall"
                    android:layout_width="50dp"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="(Hex)data" />


                <EditText
                    android:id="@+id/et_data"
                    android:layout_width="200dp"
                    android:layout_height="fill_parent"
                    android:digits="@string/hex"
                    android:gravity="center"
                    android:hint="Data(Hex)"
                    android:maxLength="16"
                    android:text="0000000000000000" />

                <EditText
                    android:id="@+id/et_time"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:gravity="center"
                    android:hint="1000"
                    android:inputType="number"
                    android:maxLength="4"
                    android:text="100" />

                <TextView
                    style="@style/text_verySmall"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="ms" />

                <Button
                    android:id="@+id/btn_send"
                    style="@style/setnt_button"
                    android:layout_width="60dp"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="@string/send" />

                <Button
                    android:id="@+id/btn_pasue"
                    style="@style/setnt_button"
                    android:layout_width="60dp"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="@string/pause" />


            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/linearLayout10"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true">

        <include
            android:id="@+id/bottomBar"
            layout="@layout/bottom_bar_back"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/tvsetting"
            style="@style/title_word"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rllll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="70dp"
        android:background="@color/back"
        android:visibility="invisible">

        <LinearLayout
            android:id="@+id/l1"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="5dp">

            <TextView
                style="@style/text_verySmall"
                android:layout_width="120dp"
                android:layout_height="80dp"

                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:text="@string/ml_tx" />

            <Spinner
                android:id="@+id/id_stype"
                style="@style/text_verySmall"
                android:layout_width="250dp"
                android:layout_height="80dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:gravity="center" />



            <Button
                android:id="@+id/mlset"
                style="@style/setnt_button"
                android:layout_width="150dp"
                android:layout_height="80dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="205dp"
                android:text="@string/mlml" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/l2"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_below="@+id/l1"
            android:layout_marginStart="20dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="5dp">

            <TextView
                style="@style/text_verySmall"
                android:layout_width="120dp"
                android:layout_height="80dp"

                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:text="@string/lx" />

            <Spinner
                android:id="@+id/lx_stype"
                style="@style/text_verySmall"
                android:layout_width="250dp"
                android:layout_height="80dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:gravity="center" />

            <LinearLayout
                android:layout_width="100dp"
                android:layout_height="80dp"
                android:layout_marginLeft="5dp"

                android:layout_toRightOf="@+id/lx_stype"
                android:gravity="center"
                android:orientation="vertical">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="当前类型"
                    android:textColor="@color/blue"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textnowstyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1000"
                    android:textColor="@color/blue"
                    android:textStyle="bold" />


            </LinearLayout>


            <Button
                android:id="@+id/btn_mlcomplete"
                style="@style/setnt_button"
                android:layout_width="150dp"
                android:layout_height="80dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="100dp"
                android:text="@string/scml" />

            <Button
                android:id="@+id/btn_sendcomplete"
                style="@style/setnt_button"
                android:layout_width="150dp"
                android:layout_height="80dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:text="@string/sendml" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/l3"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_below="@+id/l2"
            android:layout_marginStart="20dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="80dp"
                android:layout_weight="1">

                <LinearLayout
                    android:id="@+id/tt1"


                    android:layout_width="120dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/ttt1"
                        style="@style/text_verySmall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:text="@string/lx" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/lll1"
                    android:layout_width="wrap_content"
                    android:layout_height="80dp"
                    android:layout_marginLeft="20dp"
                    android:layout_toRightOf="@+id/tt1"
                    android:gravity="center_vertical">

                    <Button
                        android:id="@+id/bb1"
                        style="@style/setnt_button"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:text="-" />
                </LinearLayout>

                <SeekBar
                    android:id="@+id/sb1"
                    style="@android:style/Widget.ProgressBar.Horizontal"
                    android:layout_width="190dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"

                    android:layout_toRightOf="@+id/lll1"
                    android:max="255"
                    android:progress="0"
                    android:secondaryProgress="0" />

                <LinearLayout
                    android:id="@+id/lll1_1"
                    android:layout_width="wrap_content"
                    android:layout_height="80dp"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@+id/sb1"
                    android:gravity="center_vertical">

                    <Button
                        android:id="@+id/bb1_1"
                        style="@style/setnt_button"
                        android:layout_width="25dp"


                        android:layout_height="25dp"

                        android:text="+" />
                </LinearLayout>

